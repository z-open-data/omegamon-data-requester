    "@types/react-router-dom": "catalog:",
    "@types/testing-library__jest-dom": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "copy-webpack-plugin": "catalog:",
    "css-loader": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
